<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.pengjinfei.maven.mapper.PersonMapper">

    <update id="updateAddress" parameterType="com.pengjinfei.maven.entity.Person">
        UPDATE person p
            SET p.address='giveup'
        WHERE p.id=#{id}
    </update>
</mapper>